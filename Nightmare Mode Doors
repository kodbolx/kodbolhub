-- Fire Guardian
 
coroutine.wrap(function()
	while true do
                warn("void waiting started")
		wait(295)
		game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
		if workspace:FindFirstChild("SeekMovingNewClone") or game.ReplicatedStorage.GameData.LatestRoom.Value == 50 or game.ReplicatedStorage.GameData.LatestRoom.Value == 51 or game.ReplicatedStorage.GameData.LatestRoom.Value == 52 then  
			warn("Seek or Figure detected!")
		else
		wait()

function PurpleLights()
workspace.CurrentRooms[tostring(game.ReplicatedStorage.GameData.LatestRoom.Value)].Assets.Light_Fixtures.LightStand.LightFixture.PointLight.Color = Color3.fromRGB(128, 0, 255)
 
workspace.CurrentRooms[tostring(game.ReplicatedStorage.GameData.LatestRoom.Value)].Assets.Light_Fixtures.LightStand.LightFixture.PointLight.Enabled = true
 
workspace.CurrentRooms[tostring(game.ReplicatedStorage.GameData.LatestRoom.Value)].Assets.Light_Fixtures.LightStand.LightFixture.PointLight.Brightness = 4
 
workspace.CurrentRooms[tostring(game.ReplicatedStorage.GameData.LatestRoom.Value)].Assets.Light_Fixtures.LightStand.LightFixture.PointLight.Range = 60
 
workspace.CurrentRooms[tostring(game.ReplicatedStorage.GameData.LatestRoom.Value)].Assets.Light_Fixtures.LightStand.LightFixture.Neon.Color = Color3.fromRGB(128, 0, 255)
end

PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
PurpleLights()
