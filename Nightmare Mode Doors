-- start
 
coroutine.wrap(function()
 
game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
 
if game.ReplicatedStorage.GameData.LatestRoom.Value == 1 then
 
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Nightmare mode initiated",true)
wait(3)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Made by Kodbol (Scriptrer) and Asif (Helper)",true)
wait(3)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("This maybe hard idk",true)
wait(3)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Try hard",true)
end
 
end)

end)()
 
--
 
-- achievement script executed
 
loadstring(game:HttpGet("https://pastebin.com/raw/FYws3GiT"))()
